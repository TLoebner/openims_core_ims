<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.fhg.fokus.hss.model.Impu" 
    table="impu"
>

    <id
        name="impuId"
        type="java.lang.Integer"
        column="impu_id"
    >
        <generator class="native" />
    </id>

    <property
        name="userStatus"
        type="java.lang.String"
        column="user_status"
        not-null="true"
        length="1"
    />
    <property
        name="sipUrl"
        type="java.lang.String"
        column="sip_url"
        not-null="true"
        length="255"
    />
    <property
        name="telUrl"
        type="java.lang.String"
        column="tel_url"
        not-null="true"
        length="255"
    />
    <property
        name="barringIndication"
        type="java.lang.Boolean"
        column="barring_indication"
        not-null="true"
        length="1"
    />
    <property
        name="psi"
        type="boolean"
        column="psi"
        not-null="true"
        length="1"
    />
    
    <!-- Associations -->

    <!-- bi-directional one-to-one association to Psi -->
    <many-to-one
        name="assignedPsi"
        class="de.fhg.fokus.hss.model.Psi"
        unique="true"
        cascade="none"
        not-null="false"
    >
        <column name="psi_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Svp -->
    <many-to-one
        name="svp"
        class="de.fhg.fokus.hss.model.Svp"
        not-null="true" 
        cascade="none">
        <column name="svp_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to Impu2svpr -->
    <set
        name="impis"
        lazy="true"
        inverse="true"
		cascade="delete"
		table="impu2impi"
    >
        <key column="impu_id" />
        <many-to-many column="impi_id"
            class="de.fhg.fokus.hss.model.Impi"
        />
    </set>
    
     <!-- bi-directional one-to-many association to NotifyImsUserState -->
    <set
        name="notifyImsUserStates"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="impu_id" />
        </key>
        <one-to-many 
            class="de.fhg.fokus.hss.model.NotifyImsUserState"
        />
    </set>
    <!-- bi-directional one-to-many association to NotifyScscfname -->
    <set
        name="notifyScscfnames"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="impu_id" />
        </key>
        <one-to-many 
            class="de.fhg.fokus.hss.model.NotifyScscfname"
        />
    </set>
</class>
</hibernate-mapping>
