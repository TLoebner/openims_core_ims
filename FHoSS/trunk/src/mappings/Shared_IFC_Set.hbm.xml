<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="de.fhg.fokus.hss.db.model.Shared_IFC_Set" table="shared_ifc_set">
	
	<id name="id">
		<generator class="native"/>
	</id>

	<property name="id_set" type="int" not-null="false"/>
	<property name="name" type="string" not-null="true"/>
	<property name="priority" type="int" not-null="true"/>
	<property name="id_ifc" type="java.lang.Integer" not-null="false"/>

</class>
</hibernate-mapping>


