<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="de.fhg.fokus.hss.db.model.IMSU" table="imsu">
	
	<id name="id">
		<generator class="native"/>
	</id>

	<property name="name" type="string" not-null="true" />
	<property name="scscf_name" type="string" />
	<property name="diameter_name" type="string"/>
	<property name="id_capabilities_set" type="java.lang.Integer" />	
	<property name="id_preferred_scscf_set" type="java.lang.Integer" />	

</class>
</hibernate-mapping>
