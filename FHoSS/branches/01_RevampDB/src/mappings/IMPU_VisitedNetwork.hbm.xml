<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="de.fhg.fokus.hss.db.model.IMPU_VisitedNetwork" table="impu_visited_network">
	
	<id name="id">
		<generator class="native"/>
	</id>

    <many-to-one name="impu" class="de.fhg.fokus.hss.db.model.IMPU" not-null="true" 
    	cascade="none" lazy="true">
        <column name="id_impu" />
    </many-to-one>	

    <many-to-one name="visited_network" class="de.fhg.fokus.hss.db.model.VisitedNetwork" not-null="true" 
    	cascade="none" lazy="true">
        <column name="id_visited_network" />
    </many-to-one>	
	
</class>
</hibernate-mapping>




