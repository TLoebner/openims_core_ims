<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="de.fhg.fokus.hss.db.model.CapabilitiesSet" table="capabilities_set">
	
	<id name="id">
		<generator class="native"/>
	</id>

	<property name="id_set" type="int" not-null="true" />
	<property name="name" type="string" not-null="true"/>
	<property name="is_mandatory" type="int" />
	<property name="id_capability" type="int" />
	
</class>
</hibernate-mapping>

