<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="de.fhg.fokus.hss.db.model.IFC" table="ifc">
	
	<id name="id">
		<generator class="native"/>
	</id>
	<property name="name" type="string" not-null="true" />
	<property name="profile_part_ind" type="java.lang.Integer" not-null="false"/>
	<property name="id_application_server" type="java.lang.Integer" not-null="false"/>
	<property name="id_tp" type="java.lang.Integer" not-null="false" />
</class>
</hibernate-mapping>

