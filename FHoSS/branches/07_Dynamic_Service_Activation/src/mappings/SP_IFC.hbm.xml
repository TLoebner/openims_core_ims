<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="de.fhg.fokus.hss.db.model.SP_IFC" table="sp_ifc">
	
	<id name="id">
		<generator class="native"/>
	</id>

	<property name="priority" type="int" not-null="true"/>
	<property name="id_sp" type="int" not-null="true"/>
	<property name="id_ifc" type="int" not-null="true"/>

</class>
</hibernate-mapping>
